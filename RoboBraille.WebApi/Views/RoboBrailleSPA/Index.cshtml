@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{
    ViewBag.Title = "Hello RoboBraille";
}
<div ng-app="roboApp"style="margin-top:40px;">
    <div ng-controller="roboAppController " data-ng-init="init()" class="container main" role="main">
        <div id="robo-step-text-buttons">
            <div class="row ">
                <div class="col-sm-12">
                    <div class="content">
                        <img src="data:image/jpg;base64,"
                             style="float:right;" class="img-responsive" alt="" />
                        <h1>Convert to alternative media</h1>
                        <p>Follow the steps below to have your document converted into an alternative, accessible format.</p>
                        <h2>Select desired input</h2>
                    </div>
                </div>
            </div><!-- row-->
            <!-- Step 1 file - URL - text -->
            <div class="row btn-group btn-group-justified">
                <!-- should this have nav tag? -->

                <div class="col-sm-4">
                    <button class="btn btn-primary robo-nav-btn-font robo-nav-btn-style" ng-click="inputSelection(0)" tabindex="1">
                        <span class="glyphicon glyphicon-folder-open" aria-hidden="true"></span>
                        <br>
                        File
                    </button>
                </div>

                <div class="col-sm-4">
                    <button class="btn btn-primary robo-nav-btn-font robo-nav-btn-style" ng-click="inputSelection(1)" tabindex="2">
                        <span class="glyphicon glyphicon-globe" aria-hidden="true"></span>
                        <br>
                        URL
                    </button>
                </div>

                <div class="col-sm-4">
                    <button class="btn btn-primary robo-nav-btn-font robo-nav-btn-style" ng-click="inputSelection(2)" tabindex="3">
                        <span class="glyphicon glyphicon-text-size" aria-hidden="true"></span>
                        <br>
                        Text
                    </button>
                </div>
            </div><!-- Step 1 file - URL - text-->
        </div>
        <div id="robo-upload-options" class="robo-upload-options">
            <!-- step 1 input options-->
            <div id="robo-file-upload" ng-init="showFileUpload=true" ng-show="showFileUpload">
                <div class="form-group">
                    <label for="file-upload" class="control-label">Upload File</label>
                    <input id="file-upload" type="file" class="file" data-show-preview="false" ng-click="openDialog()" />
                </div>
            </div>
            <div id="robo-url-upload" ng-init="showUrlUpload=false" ng-show="showUrlUpload">
                <div class="form-group">
                    <label for="url-upload" class="control-label">Insert Url</label>
                    <input id="url-upload" type="text" class="form-control" ng-model="upload.url" />
                    <button ng-click="inputStepClick(1)" class="btn-primary btn-block robo-btn-form form-control">Upload</button>
                </div>
            </div>
            <div id="robo-text-upload" ng-init="showTextUpload=false" ng-show="showTextUpload">
                <div class="form-group">
                    <label for="text-upload" class="control-label">Insert Text</label>
                    <textarea id="text-upload" type="text" class="form-control" rows="5" ng-model="upload.text"></textarea>
                    <button ng-click="inputStepClick(2)" class="btn-primary btn-block robo-btn-form form-control">Upload</button>
                </div>
            </div>
        </div><!-- step 1 input options-->
        <!-- Step 2 output formats-->
        <div id="robo-step-text-buttons" ng-init="showOutputFormat=false" ng-show="showOutputFormat">
            <div class="row">
                <div class="col-sm-12">
                    <div class="content">
                        <h2>Select output format</h2>
                        <p>Specify the target format of your document.</p>
                    </div>
                </div>
            </div><!-- row-->
            <!-- Step 1 file - URL - text -->
            <div class="row btn-group btn-group-justified">
                <!-- should this have nav tag? -->

                <div class="col-sm-4">
                    <button class="btn btn-primary robo-nav-btn-font robo-nav-btn-style" ng-click="inputStepClick(3)" ng-init="showOptionAudio=false" ng-show="showOptionAudio">
                        <span class="glyphicon glyphicon-music" aria-hidden="true"></span>
                        <br>
                        Audio
                    </button>
                </div>

                <div class="col-sm-4">
                    <button class="btn btn-primary robo-nav-btn-font robo-nav-btn-style" ng-click="inputStepClick(4)" ng-init="showOptionBraille=false" ng-show="showOptionBraille">
                        <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
                        <br>
                        Braille
                    </button>
                </div>

                <div class="col-sm-4">
                    <button class="btn btn-primary robo-nav-btn-font robo-nav-btn-style" ng-click="inputStepClick(5)" ng-init="showOptionEbook=false" ng-show="showOptionEbook">
                        <span class="glyphicon glyphicon-book" aria-hidden="true"></span>
                        <br>
                        E-book
                    </button>
                </div>
                <div class="col-sm-4">
                    <button class="btn btn-primary robo-nav-btn-font robo-nav-btn-style" ng-click="inputStepClick(6)" ng-init="showOptionAC=false" ng-show="showOptionAC">
                        <span class="glyphicon glyphicon-check" aria-hidden="true"></span>
                        <br>
                        Accessibility conversion
                    </button>
                </div>
                <div class="col-sm-4">
                    <button class="btn btn-primary robo-nav-btn-font robo-nav-btn-style" ng-click="inputStepClick(7)" ng-init="showOptionAudio=Daisy" ng-show="showOptionDaisy">
                        <span class="glyphicon glyphicon-zoom-in" aria-hidden="true"></span>
                        <br>
                        Daisy
                    </button>
                </div>
            </div>
        </div><!-- Step 2 output formats end-->
        <!-- convert image to audio start-->
        <div id="robo-file-to-audio" class="row col-sm-12" ng-init="showFileToAudio=false" ng-show="showFileToAudio">
            <div class="row">
                <div class="col-sm-12">
                    <div class="content">
                        <h3>Convert to Audio</h3>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label for="audio-languages">Language</label>
                <select id="audio-languages" ng-model="audio.lang" ng-options="y.name for y in langs" ng-change="updateOptions()" class="form-control robo-select"></select>
            </div>
            <div class="form-group">
                <label for="audio-speed">Speed</label>
                <input type="range" id="audio-speed" min="-10" max="10" ng-model="audio.speed" />
            </div>
            <div class="form-group">
                <label for="output-format">Output format</label>
                <select id="output-format" ng-model="audio.output" ng-options="x.name for x in audioOutputFormat" class="form-control robo-select"></select>
            </div>
            <div class="form-group" ng-show="hasGender||dkFemales||isLithuanian">
                <div id="audio-proprieties">
                    <div ng-init="hasGenderOption=false" ng-show="hasGender">
                        <label for="gender">Voice gender</label>
                        <select id="gender" ng-model="audio.gender" ng-change="danishFemales()" class="form-control robo-select">
                            <option value="2">Female</option>
                            <option value="1">Male</option>
                        </select>
                        <!--<div class="switch switch-yes-no">
                            <input id="gender" class="switch-input" type="checkbox" ng-model="audio.gender" ng-true-value="2" ng-false-value="1" ng-change="danishFemales()" />
                            <span class="switch-label" data-on="Female" data-off="Male"></span>
                            <span class="switch-handle"></span>
                        </div>-->
                    </div>
                    <div ng-init="isLithuanian=false" ng-show="isLithuanian">
                        <label for="age">Voice age</label>
                        <select id="age" ng-model="audio.age" class="form-control robo-select">
                            <option value="4">Younger</option>
                            <option value="3">Older</option>
                        </select>
                        <!--<div class="switch switch-yes-no">
                            <input id="age" class="switch-input" type="checkbox" ng-model="audio.age" ng-true-value="4" ng-false-value="3" />
                            <span class="switch-label" data-on="Younger" data-off="Older"></span>
                            <span class="switch-handle"></span>
                        </div>-->
                    </div>
                    <div ng-init="dkFemales=false" ng-show="dkFemales">
                        <label for="voicename">Voice name</label>
                        <select id="voicename" ng-model="audio.dkfemale" class="form-control robo-select">
                            <option value="11">Anne</option>
                            <option value="0">Sara</option>
                        </select>
                        <!--<div class="switch switch-yes-no">
                            <input id="voicename" class="switch-input" type="checkbox" ng-model="audio.dkfemale" ng-true-value="11" ng-false-value="0" />
                            <span class="switch-label" data-on="Anne" data-off="Sara"></span>
                            <span class="switch-handle"></span>
                        </div>-->
                    </div>
                </div>
            </div>
            <p>
                <button ng-click="convertImageToAudio()" class="btn-primary btn-block robo-btn-form form-control">Submit Job</button>
            </p>
        </div><!-- convert image to audio end-->
        <!-- accessible conversion start-->
        <div id="robo-acc-conv" class="row col-sm-12" ng-init="showAccConv=false" ng-show="showAccConv">
            <h3>Accessible Conversion</h3>
            <div class="form-group">
                <label for="acc-formats">Target Format</label>
                <select id="acc-formats" ng-model="acc" ng-options="y.name for y in accformats" class="form-control robo-select"></select>
            </div>
            <p>
                <button ng-click="doAccessibleConversion()" class="btn-primary btn-block robo-btn-form form-control">Submit Job</button>
            </p>
        </div><!-- accessible conversion end-->
        <!-- ebook start-->
        <div id="robo-ebook-conv" class="row col-sm-12" ng-init="showEBook=false" ng-show="showEBook">
            <h3>Convert to EBook</h3>
            <div class="form-group">
                <label for="ebook-formats">Target Format</label>
                <select id="ebook-formats" ng-model="ebook" ng-options="y.name for y in ebookformats" class="form-control robo-select"></select>
            </div>
            <p>
                <button ng-click="ebookConvert()" class="btn-primary btn-block robo-btn-form form-control">Submit Job</button>
            </p>
        </div><!-- ebook end-->
        <!-- braille start-->
        <div id="robo-braille-conv" class="row col-sm-12" ng-init="showBraille=false" ng-show="showBraille">
            <h3>Convert to Braille</h3>
            <div class="form-group">
                <label for="braille-lang">Language</label>
                <select id="braille-lang" ng-model="braille.lang" ng-options="y.name for y in brailleLangs" class="form-control robo-select"></select>
            </div>
            <div class="form-group">
                <label for="braille-c">Contraction level</label>
                <select id="braille-c" ng-model="braille.contraction" ng-options="y.name for y in brlcontractionList" class="form-control robo-select"></select>
            </div>
            <div class="form-group">
                <label for="braille-opt">Braille options</label>
                <select id="braille-opt" ng-model="braille.option" ng-options="y.name for y in brlformat" class="form-control robo-select"></select>
            </div>
            <div class="form-group">
                <label for="braille-output">Character set</label>
                <select id="braille-output" ng-model="braille.charSet" ng-options="y.name for y in brlout" class="form-control robo-select"></select>
            </div>
            <div class="form-group">
                <label for="braille-path">Conversion path</label>
                <select id="braille-path" ng-model="braille.convPath" ng-options="y.name for y in brlpath" class="form-control robo-select"></select>
            </div>
            <div class="form-group">
                <div class="">
                    <label for="braille-lines" class="braille-label">Lines per page</label>
                    <input type="number" min="0" id="braille-lines" ng-model="braille.linesPerPage" />
                </div>
                <div class="">
                    <label for="braille-perline" class="braille-label">Characters per line</label>
                    <input type="number" min="0" id="braille-perline" ng-model="braille.charsPerLine"/>
                </div>
                <div class="">
                    <label for="braille-has-math" class="braille-label">Contains Math</label>
                    <input type="checkbox" id="braille-has-math" ng-model="braille.hasMath"/>
                </div>
            </div>
            <div class="form-group" ng-show="braille.hasMath">
                <label for="braille-path">Math braille code</label>
                <select id="braille-path" ng-model="braille.mathCode" ng-options="y.name for y in brlmath" class="form-control robo-select"></select>
            </div>
            <p>
                <button ng-click="brailleConvert()" class="btn-primary btn-block robo-btn-form form-control">Submit Job</button>
            </p>
        </div><!-- braille end-->
        <!-- daisy start-->
        <div id="robo-ebook-conv" class="row col-sm-12" ng-init="showDaisy=false" ng-show="showDaisy">
            <h3>Convert to Daisy</h3>
            <div class="form-group">
                <label for="daisy-lang">Language</label>
                <select id="daisy-lang" ng-model="daisy.lang" ng-options="y.name for y in daisyLangs" class="form-control robo-select"></select>
            </div>
            <p>
                <button ng-click="daisyConvert()" class="btn-primary btn-block robo-btn-form form-control">Submit Job</button>
            </p>
        </div><!-- daisy end-->

        <div id="job-information" ng-init="showFeedback=false;" ng-show="showFeedback">
            <!-- final step provide output to user and status updates-->
        <br/>
            <h3>Job information</h3>
            <div class="progress">
                <div id="jobProgress" ng-init="progressbarValue=0" class="progress-bar progress-bar-info progress-bar-striped active" role="progressbar"
                     aria-valuenow="{{progressbarValue}}" aria-valuemin="0" aria-valuemax="100" style="width:{{progressbarValue}}%">
                    {{progressbarValue}}
                </div>
            </div>
            <div class="form-group">
                <p>Job Id: {{guid}}</p>
            </div>
            <div class="form-group">
                <button ng-click="getTheJobStatus()" class="btn-info btn-block robo-btn-form form-control">Check Status</button>
                <p>Status </p>
                <div id="stats" ng-switch on="stats">
                    <div class="well well-sm" ng-switch-when="-1" style="color:red; font-weight: bold;">Failed. Conversion engine cannot convert your file to the desired output. Error: {{errormessage}}</div>
                    <div class="well well-sm" ng-switch-when="0" style="color:red; font-weight: bold;">Failed. Document Conversion Error.</div>
                    <div class="well well-sm" ng-switch-when="1" style="color:green; font-weight: bold;">Success</div>
                    <div class="well well-sm" ng-switch-when="2" style="font-weight: bold;">Conversion still processing. Press the 'Check Status' button for updates.</div>
                    <div class="well well-sm" ng-switch-default style="font-weight: bold;">Waiting...</div>
                </div>
            </div>
            <div class="form-group">
                <div ng-init="isReady=false" ng-show="isReady">
                    <label>Your file will be downloaded automatically. Click here for <a href="{{resLink}}">Manual Download</a>.</label>
                </div>
            </div>
        </div><!-- final step provide output to user and status updates-->
    </div>
</div>

@section scripts {
    @Scripts.Render("~/Scripts/modernizr-2.8.3.js")
    @Scripts.Render("https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js")
    @Scripts.Render("http://ajax.googleapis.com/ajax/libs/angularjs/1.5.8/angular.min.js")
    @Scripts.Render("~/Scripts/angular.min.js")
    @Scripts.Render("~/Scripts/angular-poller.js")
    @Scripts.Render("~/Scripts/fileinput.min.js")
    @Scripts.Render("~/Scripts/browser.js")
    @Scripts.Render("~/Scripts/FileSaver.min.js")
    @Scripts.Render("~/Scripts/app/app.js")
    @Scripts.Render("~/Scripts/app/appFactories.js")
    @Scripts.Render("~/Scripts/app/appController.js")
}
